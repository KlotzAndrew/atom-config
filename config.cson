"*":
  core:
    disabledPackages: [
      "about"
      "welcome"
    ]
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    fontSize: 11
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "9a418efc-cf69-5e15-d7f7-fa5984c6bef2"
  "git-diff": {}
  "linter-elixir-credo":
    flags: "--strict"
  "linter-rubocop":
    command: "/Users/dev/.rvm/gems/ruby-2.3.1/bin/rubocop"
  "linter-ruby-reek":
    executablePath: "/Users/dev/.rvm/gems/ruby-2.4.0/bin/reek"
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
